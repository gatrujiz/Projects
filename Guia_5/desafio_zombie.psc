////Ingrese una muestra a analizar:
	muestra = ""
	
	largoMuestra = longitud(muestra)
	n = raiz(largoMuestra)
	
	Dimension matriz[n,n]
	
	validacion = validarMuestra(muestra)
	Si validacion == Verdadero Entonces
		crearMatriz(muestra,largoMuestra,n)
